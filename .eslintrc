{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "comma-dangle": 0,
    "eqeqeq": [2, "allow-null"],
    "id-length": 0,
    "strict": ["error", "global"],
    "no-unused-expressions": [2, { "allowShortCircuit": true, "allowTernary": true }],
    "prefer-template": 0,
    "radix": 0,
    "no-nested-ternary": 0,
    "max-len": ["error", 200, 2, { "ignoreUrls": true }],
    "no-param-reassign": [2, { "props": false }],
    "no-empty": ["error", { "allowEmptyCatch": true }],
    "no-mixed-operators": ["error", { "allowSamePrecedence": true }]
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true,
    "experimentalDecorators": true,
    "modules": false
  }
}